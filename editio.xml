                        <pubPlace>Taurini</pubPlace>
                     <date>1500</date> (editio princeps)</bibl></witness>
               <witness xml:id="Maz."><abbr type="siglum">Maz.</abbr> = <bibl>Mazochiana
                        <pubPlace>Romae</pubPlace>
                     <date> 1505</date></bibl></witness>
               <witness xml:id="Sonc."><abbr type="siglum">Sonc.</abbr> = <bibl>Sonciniana
                        <pubPlace>Pisauri</pubPlace>
                     <date>1512</date></bibl></witness>
               <witness xml:id="Ald."><abbr type="siglum">Ald.</abbr> = <bibl>Aldina
                        <pubPlace>Venetiis</pubPlace>
                     <date>1518</date></bibl></witness>
               <witness xml:id="Siml."><abbr type="siglum">Siml.</abbr> = <bibl>I. Simlerus
                        <pubPlace>Basileae</pubPlace>
                     <date>1575</date></bibl></witness>
               <witness xml:id="Hess."><abbr type="siglum">Hess.</abbr> = <bibl>F. Hesselius
                        <pubPlace>Roterodami</pubPlace>
