                     <msIdentifier>
                        <settlement>Vatican City</settlement>
                        <repository>Bibliotheca Apostolica Vaticana</repository>
                        <idno>4929</idno>
                     </msIdentifier>
                     <msContents>
                        <p>ff. 188<hi rend="superscript">r</hi>–195<hi rend="superscript">r</hi>:
                                 <bibl><author>Vibii Sequestris</author>
                              <title>De fluminibus, fontibus, lacubus, nemoribus, paludibus,
                                 montibus, gentibus per litteras libellus</title></bibl></p>
                     </msContents>
                     <physDesc>
                        <handDesc hands="6">
                           <handNote xml:id="V1"><abbr type="siglum">V<hi rend="superscript"
                                 >1</hi></abbr> = Corrections by the original copyist.</handNote>
                           <handNote xml:id="V2"><abbr type="siglum">V<hi rend="superscript"
                                 >2</hi></abbr> = Corrections made by a contemporary of the original
                              copyist.</handNote>
                           <handNote xml:id="V3"><abbr type="siglum">V<hi rend="superscript"
                                 >3</hi></abbr> = A corrector from the late twelfth or early
                              thirteenth century.</handNote>
                           <handNote xml:id="V4"><abbr type="siglum">V<hi rend="superscript"
                                 >4</hi></abbr> = A hand a little later than V<hi rend="superscript"
                                 >3</hi>.</handNote>
                           <handNote xml:id="Vrec"><abbr type="siglum">V<hi rend="superscript"
                                    >c</hi></abbr> = Unknown corrector.</handNote>
                        </handDesc>
                     </physDesc>
                     <history>
                        <origin>
                           <p><origDate notBefore="0850" notAfter="0900">Middle of the ninth
                                 century</origDate>.</p>
                        </origin>
                     </history>
                  </msDesc>
               </witness>
               <witness xml:id="Petr."><abbr type="siglum">Petr.</abbr> = F. Petrarcae codex
                  deperditus</witness>
               <witness xml:id="Bocc."><abbr type="siglum">Bocc.</abbr> = <bibl><author>I.
                        Boccaccius de Certaldo</author>
                     <title>De montibus, sylvis, fontibus, lacubus, fluminibus, stagnis seu
                        paludibus et de nominibus maris liber</title></bibl>.</witness>
               <witness xml:id="R"><abbr type="siglum">R</abbr> = codex Vaticanus Reg. Lat. 1477
                  pars IIa. Dom. 1374</witness>
               <witness xml:id="A"><abbr type="siglum">A</abbr> = codex Brit. Mus. Addit. 16.986 ff.
                     42–49<hi rend="superscript">v</hi> saec. XV</witness>
               <witness xml:id="F"><abbr type="siglum">F</abbr> = codex Florentinus Bibl. Ricc.
                  1220.3 ff. 3–12<hi rend="superscript">v</hi> saec. XV</witness>
               <witness xml:id="D"><abbr type="siglum">D</abbr> = codex Vaticanus Reg. Lat. 786
                  saec. XV</witness>
               <witness xml:id="N"><abbr type="siglum">N</abbr> = codex Neapolitanus Vetus
                  Borbonicus 15 saec. XV</witness>
               <witness xml:id="Tschu."><abbr type="siglum">Tschu.</abbr> = A. Tschudii codex
                  autographus i.e. Sangallensis 663 chart. 94–102. Tschudius laudatur post Aldi ante
                  Simleri editionem.</witness>
            </listWit>
            <listWit>
